

<body>
<div class="container-fluid">
    <div class="row">
        <div class="col-sm-2">
            <div class="sidenav text-center">
            <%= image_tag("user.png",size:"100x100", :class =>"rounded-circle")%>
            <h5>Member' Name</h5><hr>
               <a href="/searching">Dashboard</a><hr>
               <a href="profile">Profile</a><hr>
               <a href="map">Maps</a><hr>
               <a href="/booking">RecentBooked</a><hr>
                <%= link_to 'Log out', log_out_path, method: :delete %>
            </div>                        
        </div>

        
        <div class="col-sm-10">
           <div  style="overflow:scroll;height:600px">
             <div class="row">
               <div class="col-sm-9">
                   <% @br.each do |br| %>
                       <% space=Space.find(br.space_id) %>
                       <% wallet=space.member_subscription %>
                       <% address=space.space_address%>
                      <div class="card mb-3" style="max-width: 1000px;">
                         <div class="row no-gutters">
                            <div class="col-md-4">
                             <%= image_tag("space.jpg",:class =>"card-img-top")%>
                            </div>
                         <div class="col-md-8">
                           <div class="card-body">
                             <h5 class="card-title">Hey! Welcome</h5>
                               <p class="card-text">These are your details. Thankyou!</p>
                                 <label for="space address">
                                 <%= address.street%>
                                 <%= address.city%>
                                 <%= address.pincode%>
                                 <%= address.state%>
                                 <%= address.house_number%>
                                </label><br>
                    
                                <label for="wallet time">Time left in wallet</label>
                                   <%= wallet.time_wallet %>
                            </div>
                          </div>
                          </div>
                      </div>
                   <% end %>
                 </div>
              </div>
                <div class="col-sm-3">
                </div>
          
          </div>
        </div>
    </div>
</div>

</body>