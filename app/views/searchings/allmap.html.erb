<div class="container-fluid ab">
    <div class="row">
    <div class="col-sm-2">
     <div class="sidenav text-center">
            <%= image_tag("user.png",size:"100x100", :class =>"rounded-circle")%>
            <h5>Member' Name</h5><hr>
               <a href="/searching">Dashboard</a><hr>
               <a href="profile">Profile</a><hr>
               <a href="map">Maps</a><hr>
               <%= link_to 'Log out', log_out_path, method: :delete %>
            </div>                        
    </div>
    <div class="col-sm-10">
     <div class="container-fluid">
 <div id="map" style='width: auto; height: 800px; position: fixed;'></div>  
 </div>
 </div>
 </div>
 </div>



<script type="text/javascript">  
handler = Gmaps.build('Google');
handler.buildMap({ provider: {}, internal: {id: 'map'}}, function(){
  markers = handler.addMarkers(<%=raw @hash.to_json %>);
  handler.bounds.extendWith(markers);
  handler.fitMapToBounds();
});
</script>


